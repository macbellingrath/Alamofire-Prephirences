use_frameworks!

def common_pods
    pod 'Alamofire'
    pod 'Prephirences'
end

target 'AlamofirePrephirences' do
    platform :ios, '9.0'
    common_pods
end

target 'AlamofirePrephirencesOSX' do
    platform :osx, '10.11'
    common_pods
end

target 'AlamofirePrephirencesTVOS' do
    platform :tvos, '9.0'
    common_pods
end
